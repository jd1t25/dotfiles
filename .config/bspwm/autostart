picom --experimental-backends -b
feh --bg-fill /mnt/win2/Wallpapers/Minimalist/astro.jpeg /mnt/win2/Wallpapers/Minimalist/moon.png
bspc monitor DP-0 -s DP-1

if [ -e /tmp/first ]
then
  dunst &
  telegram-desktop &
  discord &
  notion-app-enhanced &
  time_terminal_create.sh
  redshift &
  xset r rate 300 50
  bspc config ignore_ewmh_focus true
  xfce4-power-manager &
  nohup bash -c "sleep 5; bspc config ignore_ewmh_focus false" &
fi

rm /tmp/first
notify-send "WM Reloaded"
